<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ravikumarpatel.gym_guide_app.Exercises"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view1"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list1">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/bent_over"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="BENT OVER"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view2"
                android:layout_below="@+id/card_view1"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list2">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/cross_over"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="CROSS OVER"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>



            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view3"
                android:layout_below="@+id/card_view2"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list3">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/dumbble_bent_over"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="DUMBBLLE BENT OVER"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view4"
                android:layout_below="@+id/card_view3"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list4">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/dead_lift"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="DEAD LIFT"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view5"
                android:layout_below="@+id/card_view4"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list5">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/hyper_extension"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="HYPER EXTENSION"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view6"
                android:layout_below="@+id/card_view5"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list6">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_marginTop="5dp"
                        android:layout_height="100dp"
                        android:src="@drawable/pull_ups"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="PULL UPS"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>




            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view7"
                android:layout_below="@+id/card_view6"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list7">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_marginTop="5dp"
                        android:layout_height="100dp"
                        android:src="@drawable/close_grip_pull_down"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="CLOSE GRIP PULL DOWN"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>





            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view8"
                android:layout_below="@+id/card_view7"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list8">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="100dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/reverse_dead_lift"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="REVERSE DEAD LIFT"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>



            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view9"
                android:layout_below="@+id/card_view8"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list9">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="100dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/reverse_grip_bent_over"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="REVERSE GRIP BENT OVER"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>



            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view10"
                android:layout_below="@+id/card_view9"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list10">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="100dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/single_arm_rowing"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="SINGLE ARM ROWING"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view11"
                android:layout_below="@+id/card_view10"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list11">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/t_bar_rowing"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="T-BAR ROWING"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view12"
                android:layout_below="@+id/card_view11"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list12">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/pull_down_behind_neck"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="PULL DOWN BEHIND NECK"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:id="@+id/card_view13"
                android:layout_below="@+id/card_view12"
                android:layout_marginTop="1dp"
                card_view:cardCornerRadius="1dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:id="@+id/list13">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_marginTop="5dp"
                        android:layout_height="100dp"
                        android:src="@drawable/seated_cable_rowing"

                        />
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:text="SEATED CABLE ROWING"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColor="#000"
                        />

                </LinearLayout>


            </android.support.v7.widget.CardView>


        </RelativeLayout>

    </ScrollView>




</RelativeLayout>
